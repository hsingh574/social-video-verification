#!/usr/bin/env bash



PYTHON_LOCATION=${1:-""}
SCRIPT_LOCATION=${2:-""}
RESULTS_DIR=${3:-""}
NUM_PARTICIPANTS=${4:-""}
SAVE_DIR=${5:-""}


"${PYTHON_LOCATION}/wav2lip/bin/python3" "${SCRIPT_LOCATION}/make_plots_v2.py" --results-dir ${RESULTS_DIR} \
--num_participants ${NUM_PARTICIPANTS} --save-dir ${SAVE_DIR}